{
  "name": "yosevu",
  "version": "0.0.1",
  "private": true,
  "license": "",
  "scripts": {
    "build": "export NODE_ENV=production && postcss src/css/main.css -o public/css/main.css",
    "watch:css": "postcss src/css/main.css -o public/css/main.css --watch",
    "watch:shadow": "shadow-cljs watch app",
    "start": "run-p watch:css watch:shadow",
    "release": "yarn build && shadow-cljs release app"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.0.0",
    "autoprefixer": "^10.2.3",
    "cssnano": "^4.1.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.4",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "shadow-cljs": "2.11.15",
    "tailwindcss": "^2.0.2"
  },
  "dependencies": {
    "highlight.js": "^10.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
