;; shadow-cljs configuration
{:deps true

 :nrepl
 {:port 3333}

 :builds
 {:app {:target     :browser
        :output-dir "public/js"
        :asset-path "/js"
        :modules    {:main
                     {:init-fn app.core/start!}}
        :devtools   {:after-load app.core/start!
                     :http-root  "public"
                     :http-port  3000}}}}
